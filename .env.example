# Agent SDK Environment Variables
#
# Copy this file to .env and customize for your local environment

# LLM Configuration - OpenAI
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0.7
OPENAI_BASE_URL=
OPENAI_TIMEOUT=60
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# LLM Configuration - Anthropic
ANTHROPIC_API_KEY=your_anthropic_api_key
ANTHROPIC_MODEL=claude-3-7-sonnet-20240307
ANTHROPIC_TEMPERATURE=0.7
ANTHROPIC_BASE_URL=
ANTHROPIC_TIMEOUT=60

# Memory Configuration - Redis
REDIS_URL=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# VectorStore Configuration - Weaviate
WEAVIATE_URL=
WEAVIATE_API_KEY=
WEAVIATE_SCHEME=https
WEAVIATE_HOST=localhost:8080
WEAVIATE_CLASS_NAME=Document

# DataStore Configuration - Supabase
SUPABASE_URL=
SUPABASE_API_KEY=
SUPABASE_TABLE=documents

# Tools Configuration
# WebSearch
GOOGLE_API_KEY=
GOOGLE_SEARCH_ENGINE_ID=

# GitHub
GITHUB_TOKEN=

# Tracing Configuration
# Langfuse
LANGFUSE_ENABLED=false
LANGFUSE_SECRET_KEY=
LANGFUSE_PUBLIC_KEY=
LANGFUSE_HOST=https://cloud.langfuse.com
LANGFUSE_ENVIRONMENT=development

# OpenTelemetry
OTEL_ENABLED=false
OTEL_SERVICE_NAME=agent-sdk
OTEL_COLLECTOR_ENDPOINT=localhost:4317

# Multitenancy Configuration
MULTITENANCY_ENABLED=false
DEFAULT_ORG_ID=default

# Guardrails Configuration
GUARDRAILS_ENABLED=false
GUARDRAILS_CONFIG_PATH=
